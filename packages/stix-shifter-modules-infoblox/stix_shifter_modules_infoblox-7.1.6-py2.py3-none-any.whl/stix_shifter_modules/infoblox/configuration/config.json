{
    "connection": {
        "type": {
            "displayName": "Infoblox",
            "group": "infoblox",
            "type": "connectorType"
        },
        "host": {
            "type": "text",
            "regex": "^(([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9_:/\\-]*[a-zA-Z0-9])\\.)*([A-Za-z0-9]|[A-Za-z0-9][A-Za-z0-9_:/\\-]*[A-Za-z0-9])$"
        },
        "port": {
            "type": "number",
            "default": 443,
            "min": 1,
            "max": 65535
        },
        "help": {
            "type": "link",
            "default": "https://docs.infoblox.com/pages/viewpage.action?pageId=34510138"
        },
        "cert": {
            "type": "password",
            "optional": true
        },
        "selfSignedCert": {
            "type": "password",
            "optional": true
        },
        "options": {
            "type": "fields",
            "async_call": {
                "type": "text",
                "hidden": true,
                "optional": true
            },
            "result_limit": {
                "default": 10000,
                "min": 1,
                "max": 10000000,
                "type": "number",
                "previous": "connection.resultSizeLimit"
            },
            "batch_size": {
                "default": 2000,
                "min": 1,
                "max": 10000,
                "type": "number",
                "optional": true,
                "hidden": true
            },
            "time_range": {
                "default": 5,
                "min": 1,
                "max": 10000,
                "type": "number",
                "previous": "connection.timerange",
                "nullable": true
            },
            "timeout": {
                "default": 30,
                "min": 1,
                "max": 3600,
                "hidden": true,
                "type": "number",
                "previous": "connection.timeoutLimit"
            },
            "dialects": {
                "type": "array",
                "hidden": true,
                "optional": true
            },
            "language": {
                "type": "string",
                "default": "stix",
                "optional": true,
                "hidden": true
            },
            "validate_pattern": {
                "type": "boolean",
                "optional": true,
                "hidden": true,
                "previous": "connection.validate_pattern"
            },
            "stix_validator": {
                "type": "boolean",
                "default": false,
                "optional": true,
                "hidden": true,
                "previous": "connection.stix_validator"
            },
            "mapping": {
                "type": "json",
                "optional": true,
                "previous": "connection.mapping",
                "default": {
                    "dossierData_operators": {
                        "ComparisonExpressionOperators.And": "&",
                        "ObservationOperators.And": "&",
                        "ComparisonExpressionOperators.Or": "&",
                        "ObservationOperators.Or": "&",
                        "ComparisonComparators.Equal": "="
                    },
                    "tideDbData_from_stix_map": {
                        "ipv4-addr": {
                            "fields": {
                                "value": [
                                    "ip"
                                ]
                            }
                        },
                        "ipv6-addr": {
                            "fields": {
                                "value": [
                                    "ip"
                                ]
                            }
                        },
                        "domain-name": {
                            "fields": {
                                "value": [
                                    "domain"
                                ]
                            }
                        },
                        "email-addr": {
                            "fields": {
                                "value": [
                                    "email"
                                ]
                            }
                        },
                        "x-infoblox-threat": {
                            "fields": {
                                "id": [
                                    "id"
                                ],
                                "threat_class": [
                                    "class"
                                ],
                                "x_infoblox_confidence": [
                                    "confidence"
                                ],
                                "dga": [
                                    "dga"
                                ],
                                "expiration": [
                                    "expiration"
                                ],
                                "hash": [
                                    "hash"
                                ],
                                "host_name": [
                                    "host"
                                ],
                                "imported": [
                                    "imported"
                                ],
                                "received": [
                                    "received"
                                ],
                                "origin": [
                                    "origin"
                                ],
                                "profile": [
                                    "profile"
                                ],
                                "property": [
                                    "property"
                                ],
                                "target": [
                                    "target"
                                ],
                                "threat_level": [
                                    "threat_level"
                                ],
                                "top_level_domain": [
                                    "tld"
                                ],
                                "threat_type": [
                                    "type"
                                ],
                                "active": [
                                    "up"
                                ],
                                "derivative": [
                                    "derivative"
                                ],
                                "url": [
                                    "url"
                                ],
                                "ip_ref.value": [
                                    "ip"
                                ],
                                "domain_ref.value": [
                                    "domain"
                                ],
                                "email_ref.value": [
                                    "email"
                                ]
                            }
                        }
                    },
                    "tideDbData_operators": {
                        "ComparisonExpressionOperators.And": "&",
                        "ObservationOperators.And": "&",
                        "ComparisonExpressionOperators.Or": "&",
                        "ObservationOperators.Or": "&",
                        "ComparisonComparators.Equal": "=",
                        "ComparisonComparators.GreaterThan": "=",
                        "ComparisonComparators.GreaterThanOrEqual": "=",
                        "ComparisonComparators.LessThan": "=",
                        "ComparisonComparators.LessThanOrEqual": "=",
                        "ComparisonComparators.Like": "="
                    },
                    "dossierData_from_stix_map": {
                        "domain-name": {
                            "fields": {
                                "value": [
                                    "value"
                                ]
                            }
                        },
                        "ipv4-addr": {
                            "fields": {
                                "value": [
                                    "value"
                                ]
                            }
                        },
                        "ipv6-addr": {
                            "fields": {
                                "value": [
                                    "value"
                                ]
                            }
                        },
                        "x-infoblox-dossier-event-result-pdns": {
                            "fields": {
                                "hostname_ref.value": [
                                    "value"
                                ],
                                "ip_ref.value": [
                                    "value"
                                ]
                            }
                        }
                    },
                    "dnsEventData_operators": {
                        "ComparisonExpressionOperators.And": "&",
                        "ObservationOperators.And": "&",
                        "ComparisonExpressionOperators.Or": "&",
                        "ObservationOperators.Or": "&",
                        "ComparisonComparators.Equal": "="
                    },
                    "dnsEventData_from_stix_map": {
                        "ipv4-addr": {
                            "fields": {
                                "value": [
                                    "qip"
                                ]
                            }
                        },
                        "domain-name": {
                            "fields": {
                                "value": [
                                    "qname"
                                ]
                            }
                        },
                        "network-traffic": {
                            "fields": {
                                "src_ref.value": [
                                    "qip"
                                ],
                                "extensions.'dns-ext'.question.domain_ref.value": [
                                    "qname"
                                ]
                            }
                        },
                        "x-infoblox-dns-event": {
                            "fields": {
                                "policy_name": [
                                    "policy_name"
                                ],
                                "x_infoblox_severity": [
                                    "threat_level"
                                ],
                                "threat_class": [
                                    "threat_class"
                                ],
                                "network": [
                                    "network"
                                ]
                            }
                        }
                    },
                    "to_stix_map": {
                        "dnsEventData": {
                            "event_time": [
                                {
                                    "key": "first_observed"
                                },
                                {
                                    "key": "last_observed"
                                }
                            ],
                            "private_ip": [
                                {
                                    "key": "ipv4-addr.value",
                                    "object": "private_ip"
                                }
                            ],
                            "mac_address": [
                                {
                                    "key": "mac-addr.value",
                                    "object": "mac_address"
                                },
                                {
                                    "key": "ipv4-addr.resolves_to_refs",
                                    "object": "private_ip",
                                    "references": [
                                        "mac_address"
                                    ]
                                }
                            ],
                            "user": [
                                {
                                    "key": "user-account.user_id",
                                    "object": "user"
                                },
                                {
                                    "key": "x-infoblox-dns-event.user_ref",
                                    "object": "x_infoblox",
                                    "references": "user"
                                }
                            ],
                            "qip": [
                                {
                                    "key": "ipv4-addr.value",
                                    "object": "qip"
                                },
                                {
                                    "key": "network-traffic.src_ref",
                                    "object": "nt",
                                    "references": "qip"
                                },
                                {
                                    "key": "network-traffic.protocols",
                                    "object": "nt",
                                    "value": [
                                        "domain"
                                    ]
                                },
                                {
                                    "key": "x-infoblox-dns-event.src_ip_ref",
                                    "object": "x_infoblox",
                                    "references": "qip"
                                }
                            ],
                            "rip": [
                                {
                                    "key": "ipv4-addr.value",
                                    "object": "rip"
                                },
                                {
                                    "key": "network-traffic.extensions.dns-ext.resolved_ip_refs",
                                    "object": "nt",
                                    "references": [
                                        "rip"
                                    ]
                                }
                            ],
                            "qname": [
                                {
                                    "key": "domain-name.value",
                                    "object": "qname",
                                    "transformer": "InfobloxToDomainName"
                                },
                                {
                                    "key": "network-traffic.extensions.dns-ext.question.domain_ref",
                                    "object": "nt",
                                    "references": "qname"
                                }
                            ],
                            "qtype": [
                                {
                                    "key": "network-traffic.extensions.dns-ext.type",
                                    "object": "nt"
                                }
                            ],
                            "rcode": [
                                {
                                    "key": "network-traffic.extensions.dns-ext.response_code",
                                    "object": "nt"
                                }
                            ],
                            "rdata": [
                                {
                                    "key": "network-traffic.extensions.dns-ext.answers.data",
                                    "object": "nt"
                                }
                            ],
                            "severity": [
                                {
                                    "key": "x-infoblox-dns-event.x_infoblox_severity",
                                    "object": "x_infoblox"
                                }
                            ],
                            "tclass": [
                                {
                                    "key": "x-infoblox-dns-event.threat_class",
                                    "object": "x_infoblox"
                                }
                            ],
                            "threat_indicator": [
                                {
                                    "key": "x-infoblox-dns-event.threat_indicator",
                                    "object": "x_infoblox"
                                }
                            ],
                            "tproperty": [
                                {
                                    "key": "x-infoblox-dns-event.threat_property",
                                    "object": "x_infoblox"
                                }
                            ],
                            "policy_name": [
                                {
                                    "key": "x-infoblox-dns-event.policy_name",
                                    "object": "x_infoblox"
                                }
                            ],
                            "os_version": [
                                {
                                    "key": "x-infoblox-dns-event.os_version",
                                    "object": "x_infoblox"
                                }
                            ],
                            "network": [
                                {
                                    "key": "x-infoblox-dns-event.network",
                                    "object": "x_infoblox"
                                }
                            ],
                            "category": [
                                {
                                    "key": "x-infoblox-dns-event.category",
                                    "object": "x_infoblox"
                                }
                            ],
                            "confidence": [
                                {
                                    "key": "x-infoblox-dns-event.x_infoblox_confidence",
                                    "object": "x_infoblox"
                                }
                            ],
                            "country": [
                                {
                                    "key": "x-infoblox-dns-event.country",
                                    "object": "x_infoblox"
                                }
                            ],
                            "device": [
                                {
                                    "key": "x-infoblox-dns-event.device",
                                    "object": "x_infoblox"
                                }
                            ],
                            "dhcp_fingerprint": [
                                {
                                    "key": "x-infoblox-dns-event.dhcp_fingerprint",
                                    "object": "x_infoblox"
                                }
                            ],
                            "feed_name": [
                                {
                                    "key": "x-infoblox-dns-event.feed_name",
                                    "object": "x_infoblox"
                                }
                            ],
                            "feed_type": [
                                {
                                    "key": "x-infoblox-dns-event.feed_type",
                                    "object": "x_infoblox"
                                }
                            ]
                        },
                        "dossierData": {
                            "job": {
                                "create_time": [
                                    {
                                        "key": "first_observed"
                                    },
                                    {
                                        "key": "last_observed"
                                    }
                                ]
                            },
                            "results": {
                                "data": {
                                    "items": {
                                        "Domain": [
                                            {
                                                "key": "domain-name.value",
                                                "object": "domain"
                                            },
                                            {
                                                "key": "x-infoblox-dossier-event-result-pdns.domain_ref",
                                                "object": "dossierpdns",
                                                "references": "domain"
                                            }
                                        ],
                                        "Hostname": [
                                            {
                                                "key": "domain-name.value",
                                                "object": "hostname"
                                            },
                                            {
                                                "key": "x-infoblox-dossier-event-result-pdns.hostname_ref",
                                                "object": "dossierpdns",
                                                "references": "hostname"
                                            }
                                        ],
                                        "IP": [
                                            {
                                                "key": "ipv4-addr.value",
                                                "object": "ip"
                                            },
                                            {
                                                "key": "ipv6-addr.value",
                                                "object": "ip"
                                            },
                                            {
                                                "key": "x-infoblox-dossier-event-result-pdns.ip_ref",
                                                "object": "dossierpdns",
                                                "references": "ip"
                                            }
                                        ],
                                        "Record_Type": [
                                            {
                                                "key": "x-infoblox-dossier-event-result-pdns.record_type",
                                                "object": "dossierpdns"
                                            }
                                        ],
                                        "Last_Seen": [
                                            {
                                                "key": "x-infoblox-dossier-event-result-pdns.last_seen",
                                                "object": "dossierpdns"
                                            }
                                        ],
                                        "NameServer": [
                                            {
                                                "key": "x-infoblox-dossier-event-result-pdns.name_server",
                                                "object": "dossierpdns"
                                            }
                                        ]
                                    }
                                }
                            }
                        },
                        "tideDbData": {
                            "id": [
                                {
                                    "key": "x-infoblox-threat.id",
                                    "object": "x_infoblox"
                                }
                            ],
                            "detected": [
                                {
                                    "key": "first_observed"
                                },
                                {
                                    "key": "last_observed"
                                }
                            ],
                            "received": [
                                {
                                    "key": "x-infoblox-threat.received",
                                    "object": "x_infoblox"
                                }
                            ],
                            "class": [
                                {
                                    "key": "x-infoblox-threat.threat_class",
                                    "object": "x_infoblox"
                                }
                            ],
                            "confidence": [
                                {
                                    "key": "x-infoblox-threat.x_infoblox_confidence",
                                    "object": "x_infoblox"
                                }
                            ],
                            "dga": [
