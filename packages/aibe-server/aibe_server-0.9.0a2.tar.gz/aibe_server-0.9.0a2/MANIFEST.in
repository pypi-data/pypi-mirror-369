include README.md
include LICENSE
include pyproject.toml
include requirements.txt
recursive-include aibe_server *.py
recursive-include aibe_server/static *
include extension/manifest.json
include extension/*.js
include extension/*.html
include tests/console-runner.js
include tests/web-runner.html
recursive-include tests/framework *.js
recursive-include tests/pages *.html *.svg

# Exclude development and build artifacts
global-exclude *.pyc __pycache__
global-exclude .git*
global-exclude .venv*
global-exclude .claude_notes*
global-exclude *.bak
global-exclude *.tmp
global-exclude build/*
global-exclude dist/*
global-exclude *.egg-info/*
prune .venv
prune .venv-windows
exclude server.pid
exclude server.bat
exclude server.py
exclude install.py
exclude logging.json
