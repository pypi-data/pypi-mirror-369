{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "testcpp",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/test_cpp",
            "args": [
                "${workspaceFolder}/build/test/data",
                "1"
            ],
            "cwd": "${workspaceFolder}/build",
            "env": {
                "DFTRACER_LOG_LEVEL": "DEBUG",
                "DFTRACER_TRACE_COMPRESSION": "0",
                "LD_LIBRARY_PATH": "${workspaceFolder}/build/lib",
                "DFTRACER_DATA_DIR": "${workspaceFolder}/build/test/data",
                "DFTRACER_LOG_FILE": "${workspaceFolder}/build/test/test_cpp_basic_only",
                "DFTRACER_ENABLE": "1",
                "DFTRACER_INC_METADATA": "1",
                "LD_PRELOAD": "${workspaceFolder}/build/lib64/libdftracer_preload_dbg.so",
                "DFTRACER_INIT": "FUNCTION",
            }
        },
        {
            "name": "testpy",
            "type": "lldb",
            "request": "launch",
            "program": "/workspace/venv/bin/python3",
            "args": [
                "${workspaceFolder}/test/py/test.py",
                "--format=npz",
                "--data_dir=${workspaceFolder}/build/test/data"
            ],
            "cwd": "${workspaceFolder}/build",
            "env": {
                "DFTRACER_LOG_LEVEL": "DEBUG",
                "DFTRACER_TRACE_COMPRESSION": "1",
                "LD_LIBRARY_PATH": "${workspaceFolder}/build/lib",
                "DFTRACER_DATA_DIR": "${workspaceFolder}/build/test/data",
                "DFTRACER_LOG_FILE": "${workspaceFolder}/build/test/test_cpp_basic_only",
                "DFTRACER_ENABLE": "1",
                "DFTRACER_INC_METADATA": "1",
                "LD_PRELOAD": "${workspaceFolder}/build/lib64/libdftracer_preload_dbg.so",
                "DFTRACER_INIT": "FUNCTION",
            }
        },
        {
            "name": "dlio",
            "type": "lldb",
            "request": "launch",
            "program": "/workspace/venv/bin/python3",
            "args": [
                "/workspace/venv/lib64/python3.12/site-packages/dlio_benchmark/main.py",
                "workload=unet3d_a100",
                "++workload.workflow.generate_data=False",
                "++workload.dataset.num_files_train=32",
                "++workload.dataset.record_length_bytes=1048576",
                "++workload.dataset.data_folder=./unet3d_a100/data",
                "++workload.checkpoint.checkpoint_folder=./unet3d_a100/checkpoint",
                "++workload.train.epochs=1"
            ],
            "cwd": "${workspaceFolder}/build",
            "env": {
                "DFTRACER_LOG_LEVEL": "DEBUG",
                "DFTRACER_TRACE_COMPRESSION": "1",
                "LD_LIBRARY_PATH": "${workspaceFolder}/build/lib",
                "DFTRACER_ENABLE": "1",
                "DFTRACER_INC_METADATA": "1",
            }
        },
        {
            "name": "testc",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/test_c",
            "args": [
                "${workspaceFolder}/build/test/data",
                "1"
            ],
            "cwd": "${workspaceFolder}/build",
            "env": {
                "DFTRACER_LOG_LEVEL": "DEBUG",
                "DFTRACER_TRACE_COMPRESSION": "1",
                "LD_LIBRARY_PATH": "${workspaceFolder}/build/lib",
                "DFTRACER_DATA_DIR": "${workspaceFolder}/build/test/data",
                "DFTRACER_LOG_FILE": "${workspaceFolder}/build/test/test_c_basic_only",
                "DFTRACER_ENABLE": "1",
                "DFTRACER_INC_METADATA": "1",
                "LD_PRELOAD": "${workspaceFolder}/build/lib64/libdftracer_preload_dbg.so",
            }
        },
    ]
}