0 * * * * /bin/bash %(install_dir)s/service-config/wrapper.sh runjobs hourly
0 4 * * * /bin/bash %(install_dir)s/service-config/wrapper.sh runjobs daily
0 5 * * 0 /bin/bash %(install_dir)s/service-config/wrapper.sh runjobs weekly
0 6 1 * * /bin/bash %(install_dir)s/service-config/wrapper.sh runjobs monthly
0 7 1 1 * /bin/bash %(install_dir)s/service-config/wrapper.sh runjobs yearly
