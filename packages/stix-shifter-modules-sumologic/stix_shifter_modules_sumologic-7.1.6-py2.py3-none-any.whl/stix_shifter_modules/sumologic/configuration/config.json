{
    "connection": {
        "type": {
            "displayName": "SumoLogic",
            "group": "sumologic",
            "type": "connectorType"
        },
        "region": {
            "type": "text"
        },
        "help": {
            "type": "link",
            "default": "data-sources.html"
        },
        "options": {
            "type": "fields",
            "async_call": {
                "type": "text",
                "hidden": true,
                "optional": true
            },
            "result_limit": {
                "default": 10000,
                "min": 1,
                "max": 10000000,
                "type": "number",
                "previous": "connection.resultSizeLimit"
            },
            "batch_size": {
                "default": 2000,
                "min": 1,
                "max": 10000,
                "type": "number",
                "optional": true,
                "hidden": true
            },
            "time_range": {
                "default": 5,
                "min": 1,
                "max": 10000,
                "type": "number",
                "previous": "connection.timerange",
                "nullable": true
            },
            "timeout": {
                "default": 30,
                "min": 1,
                "max": 3600,
                "hidden": true,
                "type": "number",
                "previous": "connection.timeoutLimit"
            },
            "dialects": {
                "type": "array",
                "hidden": true,
                "optional": true
            },
            "language": {
                "type": "string",
                "default": "stix",
                "optional": true,
                "hidden": true
            },
            "validate_pattern": {
                "type": "boolean",
                "optional": true,
                "hidden": true,
                "previous": "connection.validate_pattern"
            },
            "stix_validator": {
                "type": "boolean",
                "default": false,
                "optional": true,
                "hidden": true,
                "previous": "connection.stix_validator"
            },
            "mapping": {
                "type": "json",
                "optional": true,
                "previous": "connection.mapping",
                "default": {
                    "cloud_siem_to_stix_map": {
                        "timestamp": [
                            {
                                "key": "first_observed",
                                "transformer": "EpochToTimestamp"
                            },
                            {
                                "key": "last_observed",
                                "transformer": "EpochToTimestamp"
                            }
                        ],
                        "device_ip": [
                            {
                                "key": "ipv4-addr.value",
                                "object": "host_ip"
                            }
                        ],
                        "device_natIp": [
                            {
                                "key": "ipv4-addr.value",
                                "object": "nat_ip"
                            }
                        ],
                        "dns_replyIp": [
                            {
                                "key": "ipv4-addr.value",
                                "object": "dns_ip"
                            }
                        ],
                        "dstDevice_ip": [
                            {
                                "key": "ipv4-addr.value",
                                "object": "dst_ip"
                            },
                            {
                                "key": "network-traffic.dst_ref",
                                "object": "nt",
                                "references": "dst_ip"
                            },
                            {
                                "key": "x-oca-event.network_ref",
                                "object": "event",
                                "references": "nt"
                            }
                        ],
                        "srcDevice_ip": [
                            {
                                "key": "ipv4-addr.value",
                                "object": "src_ip"
                            },
                            {
                                "key": "network-traffic.src_ref",
                                "object": "nt",
                                "references": "src_ip"
                            },
                            {
                                "key": "x-oca-event.network_ref",
                                "object": "event",
                                "references": "nt"
                            }
                        ],
                        "dstPort": [
                            {
                                "key": "network-traffic.dst_port",
                                "object": "nt"
                            }
                        ],
                        "srcPort": [
                            {
                                "key": "network-traffic.src_port",
                                "object": "nt"
                            }
                        ],
                        "ipProtocol": [
                            {
                                "key": "network-traffic.protocols",
                                "object": "nt",
                                "transformer": "FormatTCPProtocol"
                            }
                        ],
                        "dstDevice_natIp": [
                            {
                                "key": "ipv4-addr.value",
                                "object": "dst_nat"
                            }
                        ],
                        "srcDevice_natIp": [
                            {
                                "key": "ipv4-addr.value",
                                "object": "src_nat"
                            }
                        ],
                        "device_mac": [
                            {
                                "key": "mac-addr.value",
                                "object": "host_mac"
                            }
                        ],
                        "srcDevice_mac": [
                            {
                                "key": "mac-addr.value",
                                "object": "src_mac"
                            },
                            {
                                "key": "ipv4-addr.resolves_to_refs",
                                "object": "src_ip",
                                "references": [
                                    "src_mac"
                                ]
                            }
                        ],
                        "dstDevice_mac": [
                            {
                                "key": "mac-addr.value",
                                "object": "dst_mac"
                            },
                            {
                                "key": "ipv4-addr.resolves_to_refs",
                                "object": "dst_ip",
                                "references": [
                                    "dst_mac"
                                ]
                            }
                        ],
                        "baseImage": [
                            {
                                "key": "file.name",
                                "object": "file_image",
                                "transformer": "ToFileName"
                            },
                            {
                                "key": "directory.path",
                                "object": "directory_image",
                                "transformer": "ToDirectoryPath"
                            },
                            {
                                "key": "process.binary_ref",
                                "object": "process",
                                "references": "file_image"
                            },
                            {
                                "key": "file.parent_directory_ref",
                                "object": "file_image",
                                "references": "directory_image"
                            },
                            {
                                "key": "x-oca-event.process_ref",
                                "object": "event",
                                "references": "process"
                            }
                        ],
                        "commandLine": [
                            {
                                "key": "process.command_line",
                                "object": "process"
                            },
                            {
                                "key": "x-oca-event.process_ref",
                                "object": "event",
                                "references": "process"
                            }
                        ],
                        "parentBaseImage": [
                            {
                                "key": "file.name",
                                "object": "file_parent_image",
                                "transformer": "ToFileName"
                            },
                            {
                                "key": "directory.path",
                                "object": "directory_parent_image",
                                "transformer": "ToDirectoryPath"
                            },
                            {
                                "key": "file.parent_directory_ref",
                                "object": "file_parent_image",
                                "references": "directory_parent_image"
                            },
                            {
                                "key": "process.binary_ref",
                                "object": "parent_process",
                                "references": "file_parent_image"
                            },
                            {
                                "key": "process.parent_ref",
                                "object": "process",
                                "references": "parent_process"
                            },
                            {
                                "key": "x-oca-event.parent_process_ref",
                                "object": "event",
                                "references": "parent_process"
                            }
                        ],
                        "user_username": [
                            {
                                "key": "process.creator_user_ref",
                                "object": "process",
                                "references": [
                                    "user"
                                ]
                            },
                            {
                                "key": "user-account.user_id",
                                "object": "user"
                            },
                            {
                                "key": "user-account.display_name",
                                "object": "user"
                            },
                            {
                                "key": "x-oca-event.user_ref",
                                "object": "event",
                                "references": "user"
                            }
                        ],
                        "user_username_raw": [
                            {
                                "key": "process.creator_user_ref",
                                "object": "process",
                                "references": [
                                    "user"
                                ]
                            },
                            {
                                "key": "user-account.user_id",
                                "object": "user"
                            },
                            {
                                "key": "user-account.display_name",
                                "object": "user"
                            },
                            {
                                "key": "x-oca-event.user_ref",
                                "object": "event",
                                "references": "user"
                            }
                        ],
                        "fromUser_username": [
                            {
                                "key": "user-account.user_id",
                                "object": "user"
                            },
                            {
                                "key": "user-account.display_name",
                                "object": "user"
                            }
                        ],
                        "fromUser_username_raw": [
                            {
                                "key": "user-account.user_id",
                                "object": "user"
                            },
                            {
                                "key": "user-account.display_name",
                                "object": "user"
                            }
                        ],
                        "file_basename": [
                            {
                                "key": "file.name",
                                "object": "file"
                            }
                        ],
                        "file_hash_sha256": [
                            {
                                "key": "file.hashes.SHA-256",
                                "object": "file"
                            }
                        ],
                        "file_hash_md5": [
                            {
                                "key": "file.hashes.MD5",
                                "object": "file"
                            }
                        ],
                        "file_hash_sha1": [
                            {
                                "key": "file.hashes.SHA-1",
                                "object": "file"
                            }
                        ],
                        "file_path": [
                            {
                                "key": "file.name",
                                "object": "file"
                            },
                            {
                                "key": "directory.path",
                                "object": "directory",
                                "references": [
                                    "file"
                                ]
                            }
                        ],
                        "http_referer_fqdn": [
                            {
                                "key": "domain-name.value",
                                "object": "referer"
                            }
                        ],
                        "http_url_fqdn": [
                            {
                                "key": "domain-name.value",
                                "object": "http_domain"
                            }
                        ],
                        "http_url": [
                            {
                                "key": "url.value",
                                "object": "url"
                            }
                        ],
                        "targetUser_email": [
                            {
                                "key": "email-addr.value",
                                "object": "target_email"
                            }
                        ],
                        "user_email": [
                            {
                                "key": "email-addr.value",
                                "object": "user_email"
                            }
                        ],
                        "metadata_deviceEventId": [
                            {
                                "key": "x-oca-event.code",
                                "object": "event"
                            }
                        ]
                    },
                    "from_stix_map": {
                        "artifact": {
                            "fields": {
                                "payload_bin": [
                                    "_raw"
                                ]
                            }
                        },
                        "domain-name": {
                            "fields": {
                                "value": [
                                    "_sourcehost"
                                ]
                            }
                        },
                        "x-ibm-finding": {
                            "fields": {
                                "event_count": [
                                    "_messagecount"
                                ],
                                "time_observed": [
                                    "_messagetime"
                                ],
                                "src_device": [
                                    "_collector"
                                ],
                                "start": [
                                    "_receipttime"
                                ]
                            }
                        },
                        "x-oca-event": {
                            "fields": {
                                "code": [
                                    "_messageid"
                                ],
                                "created": [
                                    "_messagetime"
                                ],
                                "agent": [
                                    "_collector"
                                ],
                                "module": [
                                    "_source"
                                ],
                                "provider": [
                                    "_sourcecategory"
                                ],
                                "original_ref.payload_bin": [
                                    "_raw"
                                ],
                                "domain_ref.value": [
                                    "_sourcehost"
                                ],
                                "user_ref.account_login": [
                                    "_useremail"
                                ]
                            }
                        },
                        "x-sumologic-source": {
                            "fields": {
                                "collectorid": [
                                    "_collectorid"
                                ],
                                "sourcename": [
                                    "_sourcename"
                                ]
                            }
                        },
                        "user-account": {
                            "fields": {
                                "user_id": [
                                    "id"
                                ],
                                "account_login": [
                                    "email"
                                ],
                                "display_name": [
                                    "displayName"
                                ],
                                "account_created": [
                                    "createdAt"
                                ],
                                "account_last_login": [
                                    "lastLoginTimestamp"
                                ]
                            }
                        }
                    },
                    "operators": {
                        "ComparisonExpressionOperators.And": "AND",
                        "ComparisonExpressionOperators.Or": "OR",
                        "ComparisonComparators.Equal": "=",
                        "ComparisonComparators.NotEqual": "=",
                        "ComparisonComparators.Like": "=",
                        "ComparisonComparators.In": "OR",
                        "ObservationOperators.Or": "OR",
                        "ObservationOperators.And": "AND"
                    },
                    "cloud_siem_from_stix_map": {
                        "ipv4-addr": {
                            "fields": {
                                "value": [
                                    "device_ip",
                                    "device_natIp",
                                    "dns_replyIp",
                                    "dstDevice_ip",
                                    "srcDevice_ip",
                                    "dstDevice_natIp",
                                    "srcDevice_natIp"
                                ],
                                "resolves_to_ref.value": [
                                    "srcDevice_mac",
                                    "dstDevice_mac"
                                ]
                            }
                        },
                        "network-traffic": {
                            "fields": {
                                "dst_port": [
                                    "dstPort"
                                ],
                                "src_port": [
                                    "srcPort"
                                ],
                                "protocols": [
                                    "ipProtocol"
                                ],
                                "dst_ref.value": [
                                    "dstDevice_ip"
                                ],
                                "src_ref.value": [
                                    "srcDevice_ip"
                                ]
                            }
                        },
                        "x-oca-event": {
                            "fields": {
                                "network_ref.src_ref.value": [
                                    "srcDevice_ip"
                                ],
                                "network_ref.dst_ref.value": [
                                    "dstDevice_ip"
                                ],
                                "process_ref.binary_ref.name": [
                                    "baseImage"
                                ],
                                "process_ref.command_line": [
                                    "commandLine"
                                ],
                                "parent_process_ref.binary_ref.name": [
                                    "parentBaseImage"
                                ],
                                "user_ref.user_id": [
                                    "user_username",
                                    "user_username_raw"
                                ],
                                "code": [
                                    "metadata_deviceEventId"
                                ]
                            }
                        },
                        "mac-addr": {
                            "fields": {
                                "value": [
                                    "device_mac",
                                    "srcDevice_mac",
                                    "dstDevice_mac"
                                ]
                            }
                        },
                        "file": {
                            "fields": {
                                "name": [
                                    "baseImage",
                                    "parentBaseImage",
                                    "file_basename",
                                    "file_path"
                                ],
                                "parent_directory_ref.binary_ref.name": [
                                    "baseImage",
                                    "parentBaseImage"
                                ],
                                "hashes.SHA-256": [
                                    "file_hash_sha256"
                                ],
                                "hashes.MD5": [
                                    "file_hash_md5"
                                ],
                                "hashes.SHA-1": [
                                    "file_hash_sha1"
                                ]
                            }
                        },
                        "directory": {
                            "fields": {
                                "path": [
                                    "baseImage",
                                    "parentBaseImage",
                                    "file_path"
                                ]
                            }
                        },
                        "process": {
                            "fields": {
                                "binary_ref.name": [
                                    "baseImage",
                                    "parentBaseImage"
                                ],
                                "command_line": [
                                    "commandLine"
                                ],
                                "parent_ref.binary_ref.name": [
                                    "parentBaseImage"
                                ],
                                "creator_user_ref.user_id": [
                                    "user_username",
                                    "user_username_raw"
                                ]
                            }
                        },
                        "user-account": {
                            "fields": {
                                "user_id": [
                                    "user_username",
                                    "user_username_raw",
                                    "fromUser_username",
                                    "fromUser_username_raw"
                                ],
                                "display_name": [
                                    "user_username",
                                    "user_username_raw",
                                    "fromUser_username",
                                    "fromUser_username_raw"
                                ]
                            }
                        },
                        "domain-name": {
                            "fields": {
                                "value": [
                                    "http_referer_fqdn",
                                    "http_url_fqdn"
                                ]
                            }
                        },
                        "url": {
                            "fields": {
                                "value": [
                                    "http_url"
                                ]
                            }
                        },
                        "email-addr": {
                            "fields": {
                                "value": [
                                    "targetUser_email",
                                    "user_email"
                                ]
                            }
                        }
                    },
                    "to_stix_map": {
                        "_raw": [
                            {
                                "key": "artifact.payload_bin",
                                "transformer": "ToBase64",
                                "object": "artifact"
                            },
                            {
                                "key": "x-oca-event.original_ref",
                                "object": "x-oca-event",
                                "references": "artifact"
                            }
                        ],
                        "_sourcehost": [
                            {
                                "key": "domain-name.value",
                                "object": "domain",
                                "transformer": "ToDomainName"
                            },
                            {
                                "key": "x-oca-event.domain_ref",
                                "object": "x-oca-event",
                                "references": "domain"
                            }
                        ],
                        "_messagecount": [
                            {
                                "key": "x-ibm-finding.event_count",
                                "object": "x-ibm-finding",
                                "transformer": "ToInteger"
                            },
                            {
                                "key": "number_observed",
                                "transformer": "ToInteger"
                            }
                        ],
                        "_messagetime": [
                            {
                                "key": "x-oca-event.created",
                                "object": "x-oca-event",
                                "transformer": "EpochToTimestamp"
                            },
                            {
                                "key": "x-ibm-finding.time_observed",
                                "object": "x-ibm-finding",
                                "transformer": "EpochToTimestamp"
                            },
                            {
                                "key": "first_observed",
                                "transformer": "EpochToTimestamp"
                            },
                            {
                                "key": "last_observed",
                                "transformer": "EpochToTimestamp"
                            }
                        ],
                        "_messageid": {
                            "key": "x-oca-event.code",
                            "object": "x-oca-event"
                        },
                        "_collector": [
                            {
                                "key": "x-oca-event.agent",
                                "object": "x-oca-event"
                            },
                            {
                                "key": "x-ibm-finding.src_device",
                                "object": "x-ibm-finding"
                            }
                        ],
                        "_collectorid": {
                            "key": "x-sumologic-source.collectorid",
                            "object": "x-sumologic-source"
                        },
                        "_receipttime": {
                            "key": "x-ibm-finding.start",
                            "transformer": "EpochToTimestamp",
                            "object": "x-ibm-finding"
                        },
                        "_source": {
                            "key": "x-oca-event.module",
                            "object": "x-oca-event"
                        },
                        "_sourcecategory": {
                            "key": "x-oca-event.provider",
                            "object": "x-oca-event"
                        },
                        "_sourcename": {
                            "key": "x-sumologic-source.sourcename",
                            "object": "x-sumologic-source"
                        },
                        "id": {
                            "key": "user-account.user_id",
                            "object": "user-account"
                        },
                        "email": [
                            {
                                "key": "user-account.account_login",
                                "object": "user-account"
                            },
                            {
                                "key": "x-oca-event.user_ref",
                                "object": "x-oca-event",
                                "references": "user-account"
                            }
                        ],
                        "displayName": {
                            "key": "user-account.display_name",
                            "object": "user-account"
                        },
                        "createdAt": {
                            "key": "user-account.account_created",
                            "object": "user-account"
                        },
                        "lastLoginTimestamp": {
                            "key": "user-account.account_last_login",
                            "object": "user-account"
                        }
                    }
                }
            },
            "unmapped_fallback": {
                "type": "boolean",
                "default": false,
                "optional": true,
                "hidden": true
            },
            "stix_2.1": {
                "type": "boolean",
                "default": false,
                "optional": true,
                "hidden": true
            }
        }
    },
    "configuration": {
        "auth": {
            "type": "fields",
            "access_id": {
                "type": "password"
            },
            "access_key": {
                "type": "password"
            }
        }
    }
}