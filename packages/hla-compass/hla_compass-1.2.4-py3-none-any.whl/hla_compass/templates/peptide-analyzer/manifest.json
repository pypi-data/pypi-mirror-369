{
  "name": "peptide-analyzer",
  "version": "1.0.0",
  "type": "with-ui",
  "computeType": "lambda",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com",
    "organization": "Your Organization"
  },
  "description": "Analyzes peptide sequences and predicts HLA binding affinity",
  "inputs": {
    "type": "object",
    "properties": {
      "sequence": {
        "type": "string",
        "description": "Peptide sequence to analyze",
        "pattern": "^[ACDEFGHIKLMNPQRSTVWY]+$"
      },
      "hla_allele": {
        "type": "string",
        "description": "HLA allele (e.g., HLA-A*02:01)",
        "default": "HLA-A*02:01"
      },
      "limit": {
        "type": "integer",
        "description": "Maximum number of similar peptides to return",
        "default": 10,
        "minimum": 1,
        "maximum": 100
      }
    },
    "required": ["sequence"]
  },
  "outputs": {
    "type": "object",
    "properties": {
      "analysis": {
        "type": "object",
        "description": "Peptide analysis results"
      },
      "similar_peptides": {
        "type": "array",
        "description": "Similar peptides from database"
      },
      "predictions": {
        "type": "object",
        "description": "HLA binding predictions"
      }
    }
  },
  "permissions": {
    "database": ["read"],
    "storage": true,
    "network": []
  },
  "tags": ["peptide", "analysis", "HLA", "immunology"]
}