 include pyproject.toml
 include jupyter-config/sagemaker_jupyterlab_extension_common.json
 
 include package.json
 include install.json
 include ts*.json
 include yarn.lock
 
 graft sagemaker_jupyterlab_extension_common/labextension
 graft sagemaker_jupyterlab_extension_common/logging/schemas
 graft sagemaker_jupyterlab_extension_common/utils
 
 # Javascript files
 graft src
 graft style
 prune **/node_modules
 prune dist

 # Public distribution
 graft public_dist
 graft LICENSE
 graft THIRD-PARTY-LICENSES
 
 # Patterns to exclude from any directory
 global-exclude *~
 global-exclude *.pyc
 global-exclude *.pyo
 global-exclude .git
 global-exclude .ipynb_checkpoints

recursive-include sagemaker_jupyterlab_extension_common/botocore_model *