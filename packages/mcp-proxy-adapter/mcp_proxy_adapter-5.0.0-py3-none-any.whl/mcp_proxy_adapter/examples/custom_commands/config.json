    "file_output": true
  },
  "commands": {
    "auto_discovery": true,
    "commands_directory": "./loadable_commands",
    "catalog_directory": "./catalog",
    "plugin_servers": [
      "https://plugins.techsup.od.ua/"
    ]
  },
  "debug": {
    "enabled": false,
    "level": "WARNING"
  },
  "custom": {
    "server_name": "Custom Commands MCP Proxy Server",
    "description": "Example server with custom commands",
    "features": {
      "hooks_enabled": true,
      "custom_commands_enabled": true,
      "advanced_logging": true
    }
  },
  "test": {
    "temp_value": "test_value_123"
