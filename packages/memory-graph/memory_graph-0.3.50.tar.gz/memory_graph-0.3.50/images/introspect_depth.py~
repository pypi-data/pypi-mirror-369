
class Base:

    def __init__(self, n):
        self.elements = []
        iter = self.elements
        for i in range(n):
            iter.append([i])
            iter=iter[-1]

class A(Base):

    def __init__(self, n):
        super().__init__(n)

class B(Base):

    def __init__(self, n):
        super().__init__(n)


a = A(10)
b = B(10) 

    
        
