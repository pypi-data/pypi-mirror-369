# PyPI Publishing Configuration
# Copy this file to .env and fill in your actual tokens

# TestPyPI API Token (required for testing)
# Get from: https://test.pypi.org/manage/account/token/
TEST_PYPI_TOKEN=pypi-your-test-token-here

# Production PyPI API Token (required for production releases)
# Get from: https://pypi.org/manage/account/token/
PYPI_TOKEN=pypi-your-production-token-here

# Optional: Set to 'true' to enable verbose logging
VERBOSE=false
