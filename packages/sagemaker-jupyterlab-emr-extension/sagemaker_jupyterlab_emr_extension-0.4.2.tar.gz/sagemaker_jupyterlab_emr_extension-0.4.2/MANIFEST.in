include pyproject.toml
include jupyter-config/sagemaker_jupyterlab_emr_extension.json

include package.json
include install.json
include ts*.json
include yarn.lock

graft sagemaker_jupyterlab_emr_extension/labextension
graft sagemaker_jupyterlab_emr_extension/schemas
graft sagemaker_jupyterlab_emr_extension/utils

# Javascript files
graft src
graft style
prune **/node_modules
prune dist

# Public distribution
graft public_dist
graft LICENSE
graft THIRD-PARTY-LICENSES

# Patterns to exclude from any directory
global-exclude *~
global-exclude *.pyc
global-exclude *.pyo
global-exclude .git
global-exclude .ipynb_checkpoints

# aws service model files
recursive-include sagemaker_jupyterlab_emr_extension/botocore_model *