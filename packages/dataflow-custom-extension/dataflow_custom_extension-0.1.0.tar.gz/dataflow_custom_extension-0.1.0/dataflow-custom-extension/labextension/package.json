{
  "name": "dataflow-custom-extension",
  "version": "0.1.0",
  "description": "A JupyterLab extension for Dataflow customization",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "dataflow"
  ],
  "homepage": "",
  "bugs": {
    "url": "https://github.com/Digital-Back-Office/dataflow-jupyter-extension/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Dataflow"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "src/**/*.{ts,tsx}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/Digital-Back-Office/dataflow-jupyter-extension.git"
  },
  "scripts": {
    "build": "jlpm build:lib && jlpm build:labextension:dev",
    "build:prod": "jlpm build:lib:prod && jlpm build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc --sourceMap",
    "build:lib:prod": "tsc",
    "install:extension": "tsc && jlpm build:labextension"
  },
  "dependencies": {
    "@jupyterlab/application": "^4.4.5",
    "@jupyterlab/apputils": "^4.5.5",
    "@jupyterlab/translation": "^4.4.5"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.0.0",
    "@jupyterlab/testutils": "^4.0.0",
    "@types/node": "18.19.34",
    "css-loader": "^6.7.1",
    "npm-run-all2": "^7.0.1",
    "rimraf": "^5.0.1",
    "source-map-loader": "^1.0.2",
    "style-loader": "^3.3.1",
    "typescript": "4.9.5",
    "yjs": "13.5.40"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "dataflow-custom-extension/labextension",
    "_build": {
      "load": "static/remoteEntry.ec7ae534670fed2938ae.js",
      "extension": "./extension",
      "style": "./style"
    }
  }
}
